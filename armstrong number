#include <stdio.h>

int main()
{
     int i,n,sum=0,c;
     printf("Enter a number");
     scanf("%d",&n);
     c=n;
     while(n)
     {
         i=n%10;
         sum=sum+(i*i*i);
         n=n/10;
     }
     if(c==sum)
     printf("armstrong");
     else
     printf("not an armstrong");
     
         return 0;
}
